<main class="section">

    <h1 class="title underline">Featured posts</h1><br/><br/>
    <div class="column-view article">

        <!--forEach[trending_posts]-->
        <div class="column v3 pad">

            <article class="article">
                <span class="image hd-x">
                    <img src="<!--{FeatureImage}-->" alt="<!--{Title}-->" width="100"/>
                </span>
                <br/>
                <br/>
                <div class="column-view">
                    <div class="column">
                        <time><!--{PostTime}--></time>
                    </div>
                </div>
                <h3><!--{Title}--></h3>
                <div>
                    <p class="text"><!--{Content}--></p>
                    <a href="/read/<!--{URI}-->">Read more</a>
                </div>
            </article>

        </div>
        <!--end[trending_posts]-->

    </div>



    <br/>
    <br/>
    <br/>
 

</main>



<!--include[footer]-->